<interface>
	<name>mpower - sensor readings</name>
	<description>Queries sensor readings for Ubiquiti mPower devices</description>
	<script_path>|path_php_binary| -q |path_cacti|/scripts/mpower.php</script_path>
	<arg_index>index</arg_index>
	<arg_query>query</arg_query>
	<arg_get>get</arg_get>
	<arg_prepend>|host_hostname|</arg_prepend>
	<output_delimeter>:</output_delimeter>
	<index_order>port</index_order>
	<index_order_type>alphabetic</index_order_type>
	<index_title_format>|chosen_order_field|</index_title_format>
	<fields>
		<port>
			<name>Port</name>
			<direction>input</direction>
			<query_name>port</query_name>
		</port>
		<label>
		    <name>Label</name>
			<direction>input</direction>
			<query_name>label</query_name>
		</label>
		<power>
			<name>Power (Watts)</name>
			<direction>output</direction>
			<query_name>power</query_name>
		</power>
		<energy>
			<name>Energy (kWh)</name>
			<direction>output</direction>
			<query_name>energy</query_name>
		</energy>
		<current>
			<name>Current (Amps)</name>
			<direction>output</direction>
			<query_name>current</query_name>
		</current>
		<voltage>
			<name>Voltage</name>
			<direction>output</direction>
			<query_name>voltage</query_name>
		</voltage>
		<powerfactor>
			<name>Power Factor</name>
			<direction>output</direction>
			<query_name>powerfactor</query_name>
		</powerfactor>
	</fields>
</interface>
